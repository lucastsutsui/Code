#include <bits/stdc++.h>
using namespace std;

int main ()
{
  int n, m;
  int conth=0, contm=0, aux;

  cin >> n >> m;
  
  while (n>7){
    n = n/7; conth++;
  }

  while (m>7){
    m = m/7; contm++;
  }

  if (conth + contm > 7) cout << 0 << endl;
  else{
    long long int ret=1;
    int i=2;
    
    for (int j = 0; j < conth; j++, i++) ret *= (7-i);
    for (int j = 0; j < contm; j++, i++) ret *= (7-i);

    

    cout << ret << endl;
  }

  return 0;
}
