#include <bits/stdc++.h>
using namespace std;

bool isLeap(int y){
  return (y%400 == 0 || (y%4==0 && y%100!=0));
}

int main ()
{
  int y, next, aux;
  
  cin >> y;
  next = y+1;

  if (isLeap(y)){
    aux = 366%7;
    while (aux%7!=0 && isLeap(next)){
      next++;
      aux+=365;
      if (isLeap(next)) aux+=1;
      aux = aux%7;
  }
  else{
    aux = 365%7;
    while (aux%7!=0 && !isLeap(next)){
      next++;
      aux += 365;
      if (isLeap(next)) aux+=1;
      aux = aux%7;
    }
  }

  cout << next << endl;
  
  return 0;
}
