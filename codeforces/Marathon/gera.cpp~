#include <bits/stdc++.h>
using namespace std;
#define TAM 5000

int main ()
{
  srand(1221);
  bitset<TAM+1> v;
  char c;

  for (int i = 0; i < TAM; i++) v[i] = rand()%2;

  while (1){
    int cont = 0;
    for (int i = 0; i < TAM; i++){
      c = getchar();
      if ((c-'0') != v[i]) cont++;
      if (cont == 2000) cout << i+1 << endl;
    }
  }

  return 0;
}
  
