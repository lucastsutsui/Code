#include <bits/stdc++.h>
#define TAM 5000
#define ATTEMPT 100
#define WRONG 2000
using namespace std;

bitset<TAM+1> disc;

void update (bitset<TAM+1> &curr, int k){
  disc[k-1] = 1;
  curr[k-1] = (~curr[k-1]);

  for (int i = 0; i < k; i++){
    tab[i][curr[i]] += k;
    tab[i][curr[i]+1]++;
  }

  for (int i = 0; i < k-1; i++) curr[i] = rand()%2;
}

int main ()
{
  int res;
  int tab[TAM+1][4];
  bitset<TAM+1> curr;

  srand(12345);

  for (int i = 0; i < TAM; i++){
    tab[i][0] = tab[i][1] = tab[i][2] = tab[i][3] = 0;
    curr[i] = rand()%2;
    disc[i] = 0;
  }

  for (int i = 0; i < ATTEMPT-1; i++){
    for (int j = 0; j < TAM; j++) cout << curr[j];
    cout << endl; fflush(stdout);

    cin >> res;

    update(curr, res);
  }

  return 0;
}
