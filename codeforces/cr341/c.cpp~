#include <iostream>
#include <stdio.h>
#define max(a,b) (((a) > (b))? (a) : (b))
#define min(a,b) (((a) > (b))? (b) : (a))
#define MAX 1000000001
using namespace std;

int v[MAX];

void init (){
  for (int i = 0; i < MAX; i++)
    v[i] = i/p;
}

int main ()
{
  int n, p;
  int l[112123], r[112123];

  cin >> n >> p;

  for (int i = 0; i < n; i++)
    cin >> l[i] >> r[i];

  init();

  

  return 0;
}
